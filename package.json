{
  "name": "angular-deploy",
  "version": "0.0.0",
  "engines": {
    "node": "^18.0.0"
  },
  "scripts": {
    "ng": "ng",
    "dev": "yarn set-env:dev && ng serve",
    "preview": "yarn set-env:prod && ng serve --configuration production",
    "build": "yarn set-env:prod && ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "set-env:dev": "NODE_ENV=development node ./set-env.js",
    "set-env:prod": "NODE_ENV=production node ./set-env.js"
  },
