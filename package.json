    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "bootstrap": "^5.3.3",
    "bootstrap-datepicker": "^1.10.0",
    "bootstrap-datetime-picker": "^2.4.4",
    "d3": "^7.9.0",
    "dotenv": "^16.4.5",
    "jquery": "^3.7.1",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.30.1",
    "nvd3": "^1.8.6",
